{
  "name": "church_reactjs",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://vietjax.com/",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.3.5",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-lazy-load-image-component": "^1.5.6",
    "react-router-dom": "^6.10.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "watch-sass": "node-sass-chokidar src/ -o src/ --watch --recursive",
    "build-css": "node-sass-chokidar src/ -o src/css/",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-sass start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "node-sass-chokidar": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.61.0"
  }
}
